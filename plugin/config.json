{
    "description": "A generalized SeaweedFS Docker Volume plugin",
    "documentation": "https://docs.docker.com/engine/extend/config/",
    "entrypoint": [
        "/bin/docker-plugin-volume"
    ],
    "env": [
        {
            "name": "DEBUG",
            "settable": [
                "value"
            ],
            "value": "false",
            "description": "Enable debug output."
        }
    ],
    "interface": {
        "socket": "volumedriver.sock",
        "types": [
            "docker.volumedriver/2.0"
        ]
    },
    "linux": {
        "capabilities": [
            "CAP_SYS_ADMIN"
        ],
        "devices": [
            {
                "path": "/dev/fuse"
            }
        ]
    },
    "mounts": [
        {
            "source": "/run/docker/plugins/seaweedfs",
            "destination": "/run/docker/plugins/seaweedfs",
            "type": "bind"
        }
    ],
    "network": {
        "type": "plugin-volume-seaweedfs"
    },
    "propagatedMount": "/mnt",
    "workdir": "/"
}